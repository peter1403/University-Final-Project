<style>
  .ui-keyboard-button {
    width: 90px;
    height: 90px;
  }
  .ui-keyboard-space {
    width: 400px;
  }
  .ui-keyboard-accept {
    width: 300px;
  }
  .ui-keyboard-cancel {
    width: 300px;
  }
</style>
  </head>
  <body>
    <div id="fullPage" class="container-fluid">
      <%= render 'layouts/navBar' %>
      <div id="mainBody"class="container-fluid">
        <%= render 'layouts/navMenu' %>
        <%= yield %>
      </div>
    </div>
    <%= render 'layouts/webScript' %>
