<% provide(:title, 'User content') %>
<h1> User contents </h1>
<% if logged_in? && current_user?(@user) %>
  <%= link_to new_user_content_url(@user) do %>
    <div class="modalClick">Add new content</div>
  <% end %>
<% end %>
<%= render @contents %>
<div class="paginationSite">
  <%= link_to path_to_prev_page(@contents) do %>
    <div class="modalClick pageButton">
      Prev
    </div>
  <% end %>
  <%= link_to path_to_next_page(@contents) do %>
    <div class="modalClick pageButton">
      Next
    </div>
  <% end %>
</div>
